{
  "short_name": "React App",
  "name": "Password vault",
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },

  "version": "1.0",

  "manifest_version": 2,
  "browser_action": {
      "default_popup": "index.html"
  },

  "background": {
    "page": "background.html",
    "persistent": true
  },

  "permissions": [
    "storage"
  ]

}
